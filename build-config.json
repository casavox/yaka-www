    "fileContent": {
        "js": "(function () {\n'use strict';\n\nangular\n.module('Yaka')\n.controller('<%= module %>Controller', <%= module %>Controller);\n\nfunction <%= module %>Controller() {\n\n// SCOPE\nvar vm = this;\n}\n})();",
        "modules": "(function () {\n'use strict';\n\nangular\n.module('Yaka')\n.config(config);\n\nfunction config($stateProvider, menuProvider) {\n$stateProvider\n.state('<%= module %>', {\nurl: '/<%= module %>',\ntemplateUrl: '<%= templatePath %>',\ncontroller: '<%= module %>Controller',\ncontrollerAs: 'vm'\n})\n}\n})();",
        "service": "(function () {\n'use strict';\n\nangular\n.module('Yaka')\n.service('<%= service %>', function (abstractApiService) {\nvar root = '/api/<%= service %>/';\n\nvar filters = {limit: 40,\noffset:0\n};\n\nvar defaultFields = '';\n\nvar self = abstractApiService.getInstance(root, filters, defaultFields);\n\nreturn self;\n});\n})();"
    }
